<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BrowserActivity">

    <fragment
        android:id="@+id/page_control"
        android:name="edu.temple.webbrowserapp.PageControlFragment"
        android:layout_width="407dp"
        android:layout_height="63dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_page_control" />

    <fragment
        android:id="@+id/page_viewer"
        android:name="edu.temple.webbrowserapp.PageViewerFragment"
        android:layout_width="400dp"
        android:layout_height="648dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout="@layout/fragment_page_viewer" />
</androidx.constraintlayout.widget.ConstraintLayout>